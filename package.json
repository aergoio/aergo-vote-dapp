{
  "name": "daodapp",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "upload:test": "aws s3 sync ./dist/ s3://portal-sandbox.aergo.io/ --acl public-read",
    "upload:prod": "aws s3 sync ./dist/ s3://portal.aergo.io/ --acl public-read",
    "deploy:test": "aws cloudfront create-invalidation --distribution-id E2VNOQMARJFI48 --paths '/*'",
    "deploy:prod": "aws cloudfront create-invalidation --distribution-id E1CMD3U3ROBQ2M --paths '/*'"
  },
  "dependencies": {
    "@aergoenterprise/lib-components": "file:./aergo-enterprise-library/packages/lib-components",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@herajs/client": "^0.20.3",
    "core-js": "^2.6.5",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.8.1",
    "jsbi": "^3.1.1",
    "qrcode": "^1.4.4",
    "v-calendar": "1.0.6",
    "vue": "^2.6.10",
    "vue-analytics": "^5.18.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@mdi/font": "^4.4.95",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "typescript": "3.7.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "indent": [
        "error",
        2
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
